{
  "log": {
    "access": "/data/adb/xray_lite/logs/access.log",
    "error": "/data/adb/xray_lite/logs/error.log",
    "loglevel": "error",
    "dnsLog": true
  },
  "inbounds": [
    {
      "listen": "::",
      "port": 1536,
      "protocol": "dokodemo-door",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls",
          "quic"
        ],
        "routeOnly": false
      },
      "settings": {
        "network": "tcp,udp",
        "followRedirect": true
      },
      "streamSettings": {
        "sockopt": {
          "tproxy": "tproxy"
        }
      },
      "tag": "all-in"
    }
  ]
}